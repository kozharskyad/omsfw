{
  "clangd.arguments": [
    "-header-insertion=never",
    "-compile-commands-dir=.vscode"
  ],
  "files.associations": {
    "*.h": "objective-c",
    "*.m": "objective-c"
  },
  "lldb.displayFormat": "auto",
  "lldb.showDisassembly": "never",
  "lldb.dereferencePointers": true,
  "lldb.consoleMode": "commands"
}
